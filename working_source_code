import csv
from pylab import *

#THIS LIST HOLDS VALUES FOR CREATION OF GRAPHS AND TABLES
mydata =[]
with open("C:\Users\WILLICHIE\Desktop\Python staffs\health_record.csv","rb") as csvfile:
    spamreader = csv.DictReader(1csvfile)
        
    for row in spamreader:
            for key in row.keys():
               # print(row)                
                try:
                    row[key]=int(row[key])
                    
                except:
                    continue
            mydata.append(row)
  
#TESTING    
print mydata[0]
    
